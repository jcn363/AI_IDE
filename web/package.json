{
  "name": "rust-ai-ide-web",
  "version": "1.0.0",
  "description": "AI-Powered Rust IDE Frontend",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:analyze": "vite build --mode analyze",
    "analyze-bundle": "npm run build:analyze && echo 'Bundle analysis complete! Check dist/bundle-analysis.html'",
    "analyze-bundle:server": "vite build --mode analyze && npx serve dist -s",
    "analyze-bundle:open": "npm run build:analyze && open dist/bundle-analysis.html",
    "bundle-report": "npm run build:analyze && echo 'Bundle Report:' && ls -la dist/",
    "security:audit": "npm audit --audit-level high",
    "security:audit-fix": "npm audit fix",
    "security:audit:json": "npm audit --audit-level high --json > security-audit-report.json",
    "security:audit:fix:force": "npm audit fix --force",
    "security:audit:check": "npm audit --audit-level critical --production",
    "security:outdated": "npm outdated --json > npm-outdated-report.json",
    "security:check-all": "npm run security:audit:json && npm run security:outdated && npm run security:license-check",
    "security:license-check": "npx license-checker --json > license-report.json",
    "security:comprehensive": "npm run security:audit:json && npm run security:outdated && npm run security:license-check && npm run security:snyk-scan",
    "security:snyk-scan": "npx snyk test --json > snyk-security-report.json || echo 'Snyk scan completed with issues'",
    "security:snyk-monitor": "npx snyk monitor --json > snyk-monitor-report.json || echo 'Snyk monitor completed'",
    "security:weekly": "npm run security:comprehensive && npm run security:report",
    "security:daily": "npm run security:audit:check && npm run security:outdated",
    "security:report": "node scripts/security/generate-security-report.js",
    "security:alert": "node scripts/security/security-alert-system.js",
    "security:automated": "npm run security:comprehensive && npm run security:alert",
    "bundle:security-scan": "npm run build && npm run security:audit",
    "preview": "vite preview",
    "perf:bundle-analyze": "npm run build:analyze && node scripts/analyze-bundle.js",
    "perf:size-check": "npm run build && du -sh dist/",
    "perf:compare-builds": "node scripts/compare-builds.js",
    "perf:optimize-check": "npm run build && node scripts/optimization-check.js",
    "type-check": "tsc --noEmit",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "generate-types": "cd .. && cargo run --bin type_generator --package rust-ai-ide-shared-types --file crates/rust-ai-ide-shared-types/src/types.rs > web/src/types/generated.ts",
    "prebuild": "npm run generate-types",
    "perf:bundle-size": "node scripts/performance-monitoring/bundle-size-monitor.js",
    "perf:load-time": "node scripts/performance-monitoring/load-time-monitor.js --url $PERFORMANCE_MONITOR_URL",
    "perf:memory": "node scripts/performance-monitoring/memory-monitor.js --url $PERFORMANCE_MONITOR_URL",
    "perf:tests": "node scripts/performance-monitoring/performance-tests.js --url $PERFORMANCE_MONITOR_URL",
    "perf:benchmarks": "node scripts/performance-monitoring/performance-benchmarks.js",
    "perf:memory-leaks": "node scripts/performance-monitoring/memory-leak-detector.js --url $PERFORMANCE_MONITOR_URL",
    "perf:continuous": "node scripts/performance-monitoring/continuous-monitor.js",
    "perf:report": "node scripts/performance-monitoring/performance-report-generator.js",
    "perf:all": "npm run perf:bundle-size && npm run perf:load-time && npm run perf:memory && npm run perf:tests",
    "accessibility:test:axe": "vitest run --reporter=verbose accessibility/axe.test.js",
    "accessibility:test:lighthouse": "lighthouse http://localhost:5173 --config-path=./accessibility/lighthouse-config.json --output=json --output-path=./accessibility-reports/lighthouse-report.json --chrome-flags='--headless --no-sandbox' || echo 'Lighthouse test completed'",
    "accessibility:test:pa11y": "pa11y http://localhost:5173 --config=./accessibility/pa11y-config.json || echo 'Pa11y test completed'",
    "accessibility:test:all": "npm run accessibility:test:axe && npm run accessibility:test:lighthouse && npm run accessibility:test:pa11y",
    "accessibility:report": "node scripts/accessibility/generate-accessibility-report.js",
    "accessibility:alert": "node scripts/accessibility/accessibility-alert-system.js",
    "accessibility:monitor": "node scripts/accessibility/accessibility-monitoring.js run",
    "accessibility:monitor:status": "node scripts/accessibility/accessibility-monitoring.js status",
    "accessibility:monitor:setup": "node scripts/accessibility/accessibility-monitoring.js setup",
    "accessibility:full": "npm run accessibility:test:all && npm run accessibility:report && npm run accessibility:alert",
    "prepare": "husky"
  },
  "devDependencies": {
    "@axe-core/react": "^4.9.1",
    "@monaco-editor/loader": "^1.5.0",
    "@tauri-apps/cli": "^2.8.3",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/d3": "^7.4.3",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.5.0",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@types/resize-observer-browser": "^0.1.11",
    "@types/semver": "^7.7.0",
    "@types/styled-jsx": "3.4.4",
    "@types/testing-library__react": "^10.2.0",
    "@types/three": "^0.158.0",
    "@typescript-eslint/eslint-plugin": "^8.15.0",
    "@typescript-eslint/parser": "^8.15.0",
    "@typescript-eslint/utils": "^8.15.0",
    "@vitejs/plugin-react": "^5.0.1",
    "@vitejs/plugin-react-swc": "^4.0.1",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitest/ui": "^3.2.4",
    "@wasm-tool/wasm-pack-plugin": "^1.7.0",
    "antd-style": "^3.7.1",
    "axe-core": "^4.9.1",
    "babel-jest": "^30.0.5",
    "eslint": "^9.17.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.0.0",
    "husky": "^9.1.7",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^30.0.5",
    "jest-environment-jsdom": "^30.0.5",
    "lighthouse": "^12.2.0",
    "pa11y": "^6.2.3",
    "puppeteer": "^22.15.0",
    "rollup-plugin-visualizer": "^5.12.0",
    "serve": "^14.2.4",
    "typescript": "^5.9.2",
    "vite": "^7.1.5",
    "vite-plugin-node-polyfills": "^0.24.0",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "@ant-design/icons": "^6.0.0",
    "@codingame/monaco-languageclient": "^0.17.4",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@fontsource/roboto": "^5.2.6",
    "@monaco-editor/react": "^4.7.0",
    "@mui/icons-material": "^7.3.1",
    "@mui/material": "^7.3.1",
    "@mui/system": "^7.3.1",
    "@reduxjs/toolkit": "^2.8.2",
    "@tauri-apps/api": "^2.8.0",
    "@tauri-apps/plugin-dialog": "^2.3.3",
    "@tauri-apps/plugin-fs": "^2.4.2",
    "@types/dompurify": "^3.2.0",
    "@types/react-syntax-highlighter": "^15.5.13",
    "@types/uuid": "^10.0.0",
    "@types/vscode": "^1.103.0",
    "@vscode/wasm-wasi": "^1.0.1",
    "antd": "^5.27.1",
    "core-js": "^3.45.1",
    "d3": "^7.9.0",
    "dompurify": "^3.2.6",
    "material": "^0.9.24",
    "monaco-editor": "^0.52.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-markdown": "^10.1.0",
    "react-redux": "^9.2.0",
    "react-router-dom": "^7.8.2",
    "react-syntax-highlighter": "^5.8.0",
    "react-virtuoso": "^4.6.2",
    "react-window": "^1.8.8",
    "save-svg-as-png": "^1.4.17",
    "three": "^0.170.0",
    "use-debounce": "^10.0.5",
    "uuid": "^11.1.0",
    "vscode-jsonrpc": "^8.2.1",
    "vscode-languageserver": "^9.0.1",
    "vscode-languageserver-protocol": "^3.17.5",
    "vscode-languageserver-types": "^3.17.5",
    "vscode-ws-jsonrpc": "^3.5.0",
    "web-vitals": "^3.5.0"
  }
}
