{
  "name": "rust-ai-ide-web",
  "version": "1.0.0",
  "description": "AI-Powered Rust IDE Frontend",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:analyze": "vite build --mode analyze",
    "analyze-bundle": "npm run build:analyze && echo 'Bundle analysis complete! Check dist/bundle-analysis.html'",
    "analyze-bundle:server": "vite build --mode analyze && npx serve dist -s",
    "analyze-bundle:open": "npm run build:analyze && open dist/bundle-analysis.html",
    "bundle-report": "npm run build:analyze && echo 'Bundle Report:' && ls -la dist/",
    "security:audit": "npm audit --audit-level high",
    "security:audit-fix": "npm audit fix",
    "bundle:security-scan": "npm run build && npm run security:audit",
    "preview": "vite preview",
    "type-check": "tsc --noEmit",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "generate-types": "cd ../.. && cargo run --bin type_generator --package rust-ai-ide-shared-types --file crates/rust-ai-ide-shared-types/src/types.rs > web/src/types/generated.ts",
    "prebuild": "npm run generate-types"
  },
  "devDependencies": {
    "@babel/core": "^7.28.3",
    "@babel/preset-env": "^7.28.3",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.17.0",
    "@iarna/toml": "^2.2.5",
    "@monaco-editor/loader": "^1.5.0",
    "@tauri-apps/cli": "^2.8.3",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/d3": "^7.4.3",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.3.0",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@types/resize-observer-browser": "^0.1.11",
    "@types/semver": "^7.7.0",
    "@types/styled-jsx": "3.4.4",
    "@types/testing-library__react": "^10.2.0",
    "@types/three": "^0.158.0",
    "@typescript-eslint/eslint-plugin": "^8.15.0",
    "@typescript-eslint/parser": "^8.15.0",
    "@typescript-eslint/utils": "^8.15.0",
    "@vitejs/plugin-react": "^5.0.1",
    "@vitejs/plugin-react-swc": "^4.0.1",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitest/ui": "^3.2.4",
    "@wasm-tool/wasm-pack-plugin": "^1.7.0",
    "antd-style": "^3.7.1",
    "babel-jest": "^30.0.5",
    "eslint": "^9.17.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^30.0.5",
    "jest-environment-jsdom": "^30.0.5",
    "rollup-plugin-visualizer": "^5.12.0",
    "serve": "^14.2.4",
    "typescript": "^5.9.2",
    "vite": "^7.1.3",
    "vite-plugin-node-polyfills": "^0.24.0",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "@ant-design/icons": "^6.0.0",
